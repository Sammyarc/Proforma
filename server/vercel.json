{
  "version": 2,
  "builds": [
    { "src": "server.js", "use": "@vercel/node" }
  ],
  "routes": [
    { 
      "src": "/api/(.*)",
      "headers": {
        "Access-Control-Allow-Origin": "https://proforma-gen.vercel.app",
        "Access-Control-Allow-Methods": "GET, POST, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type"
      },
      "dest": "/server.js"
    },
    { "src": "/(.*)", "dest": "/server.js" }
  ]
}